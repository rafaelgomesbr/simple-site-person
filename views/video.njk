<!--Extendendo conteudo da pagina layout-->

 
<!--Levando esse conteudo para pagina layout-->
{% block content %}
    <div id="container">
        <h1>{{item.title}}</h1>
    </div>

    <section class="video">

      
            <div class="card" id="{{item.id}}">
                <div class="card_video-Container">
                     <iframe controls="Autoplay" src="https://www.youtube.com/embed/{{item.id}}?autoplay=1&showinfo=0&fs=1&rel=0&iv_load_policy=3&allowfullscreen=1" frameborder="0"></iframe>
       

                </div>

                <div class="card_content">
                    <p> </p>
                </div>
 
            </div>
     

    </section>
 
 
{% endblock %}